class Solution {
public:
    vector‹vector<string>> groupAnagrams(vector<string>& strs) {
        map<string, vector‹string>> m;
        int n = strs.size();
        for (int i = 0; i‹n; i++)(
             strings = strs[i];
             sort(s.begin(),s. end());
             m[s].push_back(strs[i]);
         }
         vector‹vector<string>> ans;
         for (auto& it : m)(
              ans. push_back(it.second);
         }
         return ans;
     }
};
